    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverage": true,
    "mapCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,vue}"
